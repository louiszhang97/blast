<header class="navbar navbar-fixed-top navbar-inverse">
      <div class="container">
        <%= link_to "Lambdatime", '#', id: "logo" %>
        <nav>
          <ul class="nav navbar-nav navbar-right">
            <li> <a href = "/static_pages/home"> Home</a> </li>
            <li> <a href = "/members"> Members </a> </li>
            <li><a href = "/messages"> Messages </a> </li>
          </ul>
        </nav>
      </div>
</header>
<p id="notice"><%= notice %></p>

<h1>Members</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.name %></td>
        <td><%= member.phone %></td>
        <td><%= link_to 'Show', member %></td>
        <!-- <td><%= link_to 'Edit', edit_member_path(member) %></td> -->
        <td><%= link_to 'Destroy', member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<a class= "btn btn-lg btn-success"  href = "/members/new"> New Member </a>
